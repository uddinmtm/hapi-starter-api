{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "linebreak-style": "off",
      "no-console": "off",
      "import/no-extraneous-dependencies": ["error", {
          "devDependencies": true,
          "optionalDependencies": false,
          "peerDependencies": false,
          "bundledDependencies": false
      }]
  }
}